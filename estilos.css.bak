#BreadcrumbMenu1
{
   position:absolute;left:5px;
   top:558px;
   width:280px;
   height:20px;
   vertical-align:top;
   padding: 2px 5px 2px 5px;
   list-style: none;
   background-color: transparent;
   background-image: none;
   border: 0px solid transparent;
   border-radius: 0px;
   font-family: "Barlow Condensed";
   font-weight: 500;
   font-style: normal;
   font-size: 0;
   margin: 0;
   text-align: left;
   z-index:1;
}
#BreadcrumbMenu1 > li
{
   display: inline-block;
   font-size: 13px;
}
#BreadcrumbMenu1 a
{
   color: #A9A9A9;
   text-decoration: none;
}
#BreadcrumbMenu1 a:hover
{
   color: #CCFF33;
   text-decoration: none;
}
#BreadcrumbMenu1 > .active
{
   color: #CCFF33;
}
#BreadcrumbMenu1 > .active a
{
   color: inherit;
   pointer-events: none;
}
#BreadcrumbMenu1 > li + li:before
{
   content: "|";
   padding: 0 5px;
   color: #C0C0C0;
}
